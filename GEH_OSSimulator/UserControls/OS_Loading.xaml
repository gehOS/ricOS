<UserControl x:Class="GEH_OSSimulator.UserControls.OS_Loading"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GEH_OSSimulator.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="gridOS" HorizontalAlignment="Left" Background="Black" Initialized="gridOS_Initialized">
        <Image x:Name="Imagenes_RickOS2_png" Margin="23.334,81.333,20.666,108" Source="Imagenes/RickOS2.png" Stretch="Fill" HorizontalAlignment="Center"/>
        <ProgressBar x:Name="loadingBar" Height="10" Margin="51.333,0,51,69.334" VerticalAlignment="Bottom" Maximum="2000"/>
        <Label x:Name="loadingText" Content="Label" Margin="178.332,0,171.673,40.534" VerticalAlignment="Bottom" Foreground="White" FontFamily="Arial" HorizontalContentAlignment="Center"/>
    </Grid>
</UserControl>
