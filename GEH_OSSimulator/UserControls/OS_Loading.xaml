<UserControl x:Class="GEH_OSSimulator.UserControls.OS_Loading"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GEH_OSSimulator.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="gridOS" Background="Black" Initialized="gridOS_Initialized">
        <ProgressBar x:Name="loadingBar" Height="10" Margin="51.333,0,51,69.334" VerticalAlignment="Bottom" Maximum="2000"/>
        <Label x:Name="loadingText" Content="Label" Margin="178.332,0,171.673,40.534" VerticalAlignment="Bottom" Foreground="White" FontFamily="Arial" HorizontalContentAlignment="Center"/>
        <Label x:Name="LabelRicOS" Content="RicOS" HorizontalAlignment="Center" Margin="6,83,0,0" VerticalAlignment="Top" FontFamily="Arial" FontSize="96" Foreground="White"/>
        <Label x:Name="labelBienvenido" Content="Bienvenido Ricardo M." HorizontalAlignment="Center" Margin="10,120,0,0" VerticalAlignment="Center" Foreground="White" FontFamily="Arial" FontSize="26.667"/>
    </Grid>
</UserControl>
