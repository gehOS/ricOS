<UserControl x:Class="GEH_OSSimulator.UserControls.Programas.RunApp"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GEH_OSSimulator.UserControls.Programas"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="GridRun" Background="White">
        <TextBlock x:Name="textBlockRun" HorizontalAlignment="Left" TextWrapping="Wrap" VerticalAlignment="Top" FontSize="18.667" Width="{Binding ActualWidth, ElementName=GridRun}" TextAlignment="Center" Background="#FFCFE7EE"><Run Text="Run"/></TextBlock>
        <Image x:Name="___GEH_OSSimulator_component_UserControls_Imagenes_Run_png" Margin="99.338,29.83,100.662,0" Source="/GEH_OSSimulator;component/UserControls/Imagenes/Run.png" Stretch="Fill" Height="100" VerticalAlignment="Top" HorizontalAlignment="Center"/>
        <TextBox x:Name="textBox" Margin="74.503,134.83,73.722,142.17" TextWrapping="Wrap" Text="Run..."/>
        <Button x:Name="button" Content="Run!" Margin="109.272,0,115.728,91.298" VerticalAlignment="Bottom"/>
        <Button x:Name="button1" Content="X" HorizontalAlignment="Right" VerticalAlignment="Top" Width="29.305" Height="24.83"/>

    </Grid>
</UserControl>
